      limits = makeDynFloat(alarm, warning);
      fwAlertConfig_set(dpel, DPCONFIG_ALERT_NONBINARYSIGNAL, alertTexts, limits, alertClasses, summary, alertPanel, alertPanelParameters, alertHelp, exceptionInfo);   
      //if(fwConfigurationDB_handleErrors(exceptionInfo)) return;
      fwAlertConfig_activate(dpel, exceptionInfo);            
    }
    else if(type == 3){
      
      string dpel = dpNames(RPCGasGetSysName()+dp, \"RPCGasParameters\");
  //    DebugN(dpel);
      
      dyn_string exceptionInfo;
      dyn_float limits;
      dyn_string alertTexts = makeDynString(\"Error\", \"warning\", \"Ok\");
      dyn_string alertClasses = makeDynString(\"_fwErrorAck.\", \"_fwWarningAck.\", \"\");
