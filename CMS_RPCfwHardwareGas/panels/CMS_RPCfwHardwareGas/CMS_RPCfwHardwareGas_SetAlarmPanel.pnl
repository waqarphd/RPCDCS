"main()
{
  info.visible(FALSE);  
  string service;
  float alarmS, warningS;
  int type;
  getValue(\"Service\", \"text\", service);
  getValue(\"AlarmS\", \"text\", alarmS);
  getValue(\"WarningS\", \"text\", warningS);
      
  if(service == \"Mixer_C2H2F4Ratio\") type = 3;
  else if(service == \"Mixer_IC4H10Ratio\") type = 3;
  else if(service == \"Mixer_SF6Ratio\") type = 3;
  else if(service == \"Mixer_C2H2F4Input\") type = 4;
  else if(service == \"Mixer_IC4H10Input\") type = 4;
  else if(service == \"Mixer_SF6Input\") type = 4;
  else if(service == \"Mixer_OutPressure\") type = 5;  
  else if(service == \"Mixer_TotalFlow\") type = 4;
  else if(service == \"Humidifier_WaterTankLevel\") type = 6;
  
  
  
  
  dyn_string dpels = dpNames(RPCGasGetSysName()+ service, \"RPCGasParameters\");
  
  if(dynlen(dpels)==0) info.visible(TRUE);
  else{
    for(int i=1; i<=dynlen(dpels); i++){
      string check = dpSubStr(dpels[i], DPSUB_DP_EL);
      for(int j=0; j<AlarmTable.lineCount(); j++){
        string dp;
        getValue(\"AlarmTable\", \"cellValueRC\", j, \"datapoint\", dp);
        if(check == dp) AlarmTable.deleteLineN(j);
        }
      AlarmTable.appendLine(\"datapoint\", dpSubStr(dpels[i], DPSUB_DP_EL)+\".value\", \"alarm\", alarmS, \"warning\", warningS, \"type\", type);
    }    
  } 
}" 0
