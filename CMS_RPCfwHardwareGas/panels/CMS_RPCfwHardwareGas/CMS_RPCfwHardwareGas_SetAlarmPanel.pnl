"main()
{
  for(int i=0; i<AlarmTable.lineCount(); i++){
    
    string dp;
    float alarm, warning;
    int type;
    getValue(\"AlarmTable\", \"cellValueRC\", i, \"datapoint\", dp);
    getValue(\"AlarmTable\", \"cellValueRC\", i, \"alarm\", alarm);
    getValue(\"AlarmTable\", \"cellValueRC\", i, \"warning\", warning);
    getValue(\"AlarmTable\", \"cellValueRC\", i, \"type\", type);
    
 //   DebugN(dp, alarm, warning, type);
    
    if(type == 1){
  
      string dpel = dpNames(RPCGasGetSysName()+dp, \"RPCGasChannel\");
 //     DebugN(dpel);
      
      dyn_string exceptionInfo;
      dyn_float limits;
      dyn_string alertTexts = makeDynString(\"LowFlow\", \"Warning\", \"Ok\");
      dyn_string alertClasses = makeDynString(\"_fwErrorAck.\", \"_fwWarningAck.\", \"\");
      string alertPanel, alertHelp; 
      dyn_string summary, alertPanelParameters;
         
      fwAlertConfig_deactivate(dpel, exceptionInfo);
      limits = makeDynFloat(alarm, warning);
      fwAlertConfig_set(dpel, DPCONFIG_ALERT_NONBINARYSIGNAL, alertTexts, limits, alertClasses, summary, alertPanel, alertPanelParameters, alertHelp, exceptionInfo);   
      //if(fwConfigurationDB_handleErrors(exceptionInfo)) return;
      fwAlertConfig_activate(dpel, exceptionInfo);
    }
    
    else if(type == 2){

      string dpel = dpNames(RPCGasGetSysName()+dp, \"RPCGasChannel\");
//      DebugN(dpel);
      
      dyn_string exceptionInfo;
      dyn_float limits;
      dyn_string alertTexts = makeDynString(\"High\", \"Low\", \"Ok\");
      dyn_string alertClasses = makeDynString(\"_fwErrorAck.\", \"_fwWarningAck.\", \"\");
