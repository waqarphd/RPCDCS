"main()
{
  info.visible(FALSE);  
  string wheel, sector, chamber, flow;
  float alarm, warning;
  int type;
  getValue(\"Wheel\", \"text\", wheel);
  getValue(\"Sector\", \"text\", sector);
  getValue(\"ChamberW\", \"text\", chamber);
  getValue(\"InorOutW\", \"text\", flow);
  getValue(\"AlarmW\", \"text\", alarm);
  getValue(\"WarningW\", \"text\", warning);
  
  if(flow == \"difference\") type = 2;
  else type = 1;
  if(sector == \"All Sectors\") sector = \"*\";
  if(chamber == \"All Chambers\") chamber = \"*\";
    
  dyn_string wheelChamber = wheel+\"_\"+sector+\"_\"+chamber;
  dyn_string dpels = dpNames(RPCGasGetSysName()+ wheelChamber, \"RPCGasChannel\");

  if(dynlen(dpels)==0) info.visible(TRUE);
  else{
    for(int i=1; i<=dynlen(dpels); i++){
      string check = dpSubStr(dpels[i]+\".\"+flow, DPSUB_DP_EL);
      for(int j=0; j<AlarmTable.lineCount(); j++){
          string dp;
          getValue(\"AlarmTable\", \"cellValueRC\", j, \"datapoint\", dp);
          if(check == dp) AlarmTable.deleteLineN(j);
        }   
      AlarmTable.appendLine(\"datapoint\", dpSubStr(dpels[i]+\".\"+flow, DPSUB_DP_EL), \"alarm\", alarm, \"warning\", warning, \"type\", type);
    }
  }
}" 0
