dyn_float CalcOfffit(dyn_dyn_float xy)
{
 

  dyn_float ym;

  dyn_float ty;
  
  int tot = dynlen(xy);
  int rows = dynlen(xy[1]);
  
 
    for(int j = 1; j<=rows;j++)
    {
      for(int i =1 ; i<=tot;i++)
      {
      ym[j]= ym[j]+xy[i][j];
      }
    }
  
  
  // the first element is the common x and then the Y (HV channels starting from ch0)
  
  for(int i = 1; i<=dynlen(ym);i++)
    ym[i]=ym[i]/tot;
  //DebugN(\"media:\",ym);
  float tx,temp;
  dyn_float sum;
  dyn_float ty;
  dyn_string offf;
  float sx;
  dyn_float sy,sxy;
  
  for(int i = 1;i<=tot;i++)
  {
    
    tx = (xy[i][1]-ym[1]);
    for(int j = 2;j<=dynlen(xy[i]);j++)
    {
      dynAppend(ty,(xy[i][j]-ym[j]));
      //DebugN(xy[i][j],ym[j]);
      }
   
     sx = sx + pow(tx,2);
     
    for(int j = 1;j<=dynlen(ty);j++)
    {
     sy[j] = sy[j] + pow(ty[j],2);
     sxy[j] = sxy[j] + tx * ty[j] ;
    }
    dynClear(ty);
    
    
  }
  //DebugN(\"\");
  //DebugN(tot,\">\",dynlen(sxy));
  for(int i = 1; i<=dynlen(sxy);i++)
  {
    temp = ym[i+1] - (sxy[i]/sx) * ym[1];
    sprintf(offf[i],\"%6.1f\",temp);
  }
   
  return offf; 

}

dyn_string CalcOff(dyn_dyn_float xy)
{
  //DebugN(xy);
  dyn_dyn_float temp;
  dyn_float tvect;
  dyn_string offsets;
  float div;
  for(int i = 2;i<=dynlen(xy);i++)
  {
    for(int j= 2;j<=dynlen(xy[i]);j++)
    {
      div = (xy[i][1]-xy[1][1]);
      if(div<=0)
      {
        DebugN(\"Error\");
        div = 1;
        }
        dynAppend(tvect,(xy[i][j]-xy[1][j])/div);
      }
     dynAppend(temp,tvect);
     dynClear(tvect);  
    }
  
  int len = (dynlen(xy)-1);
  for(int i = len;i>=2;i--)
  {
    for(int j= 2;j<=dynlen(xy[i]);j++)
    {
      div = (xy[len+1][1]-xy[i][1]);
      if(div<=0)
      {
        DebugN(\"Error\");
        div = 1;
        }
        dynAppend(tvect,(xy[len+1][j]-xy[i][j])/div);
      }
     dynAppend(temp,tvect);
     dynClear(tvect);  
    }
  tvect = makeDynFloat(0,0,0,0,0,0);
  for(int i = 1; i<=dynlen(temp);i++)
  {
    for(int j = 1;j<=dynlen(temp[i]);j++)
    {
      tvect[j]= tvect[j]+temp[i][j];
      }
   }
  len = dynlen(temp);
  dyn_dyn_float values;
  //q = y - mx
  for(int k=1;k<=dynlen(xy);k++)
    for(int i = 1; i<=dynlen(tvect);i++)
     values[k][i] = xy[k][i+1]-(tvect[i]/len)*xy[k][1];
  dyn_float offs;
//       sprintf(offsets[i],\"%6.2f\",(xy[k][i+1]-(tvect[i]/len)*xy[k][1]));
     
  for(int i = 1; i<=dynlen(values);i++)
   for(int k = 1; k<=dynlen(values[i]);k++)
        offs[k] = offs[k] + values[i][k];
  //DebugN(\"M: \",temp);   
  //DebugN(\"Offser\",offs,\"vari q:\",values);   
  for(int i = 1; i<=dynlen(offs);i++)
    sprintf(offsets[i],\"%6.1f\",offs[i]/dynlen(values));
  
  //DebugN(temp);
  return offsets;
  
  }

getAvarage(dyn_float & values)
{
float sum = 0;
float val;
delay(10,0);
for(int i =1; i<=dynlen(channels);i++)
{  
  dpGet(channels[i]+\".actual.iMon\",val);
  dynAppend(values,val);
  
  }
}
    
stepChan(int val)
{
  bool onoff=false;
  int rb;
if(!stop)
  {
for(int i =1; i<=dynlen(channels);i++)
{
  dpSet(channels[i]+\".settings.i0\",50);
  dpSet(channels[i]+\".settings.rDwn\",10);
  dpSet(channels[i]+\".settings.v0\",val);
  dpGet(channels[i]+\".actual.isOn\",onoff);
  if(!onoff)
    dpSet(channels[i]+\".settings.onOff\",true);
}
dpGet(channels[dynlen(channels)]+\".readBackSettings.v0\",rb);  
while(rb!=val)
{
  delay(2,0);
  dpGet(channels[dynlen(channels)]+\".readBackSettings.v0\",rb);  
  
  }
}
}

bool iStable(int val)
{
delay(dela,0);    
int status;
float vMon;
bool ready = true;
if(!stop)
{
for(int i =1; i<=dynlen(channels);i++)
  {
  dpGet(channels[i]+\".actual.status\",status);
  dpGet(channels[i]+\".actual.vMon\",vMon);
  Debug(status, \"  \");
  if((((val-vMon)>20)||((vMon-val)>20))||(status>1))
    ready = false;       
  }
DebugN(\"\");
}
return ready;
