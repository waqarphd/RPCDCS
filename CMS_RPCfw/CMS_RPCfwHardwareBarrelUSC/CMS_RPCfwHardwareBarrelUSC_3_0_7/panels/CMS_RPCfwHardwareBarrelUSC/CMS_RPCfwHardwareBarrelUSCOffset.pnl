string getDp(int num){
  dyn_string channels = dpNames(\"*\",\"RpcBoardOffset\");
  //DebugN(channels);
  bool find = false;
  string channel;
  int i = 1;
  string serial,empty;
  while(!find)
  {
    dpGet(sysName+channels[i]+\".serial\",serial);
    //DebugN(serial,\"<>\",num);
    if(serial == num)
    {
      find = true;
      channel = channels[i];
    }else
    { 
     if((serial == \"\")&&(empty == \"\"))
       empty = i;
       i++;
     }
    if(empty !=\"\")
      {
      
      find = true;
      DebugN(\"Serial not existed yet, creating at \"+ empty);
      dpSet(sysName+channels[empty]+\".serial\", num);
      setArch(sysName+channels[empty]+\".offset\");
      channel = sysName+channels[empty];
      }    
    }
  //DebugN(channel,num,\"dddd\");
  return channel;
  
  }

