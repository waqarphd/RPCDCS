scanhv(string dp, float fNewValue)
{
  int rc,volt;
   
    rc=dpGet(RPCGGM_getSysName()+\"CAEN/GGM/board00/channel002.settings.v0\",volt);
  
    if (volt > 6000)
    {
      scan_label.text=(\"GGM is performing the scan at \"+volt);
      volt=volt-50;
    }
    if (volt == 100)
    {
      PanelOff();
    }
    
    dpSetWait(RPCGGM_getSysName()+\"CAEN/GGM/board00/channel002.settings.v0\", volt);
    dpSetWait(RPCGGM_getSysName()+\"CAEN/GGM/board00/channel003.settings.v0\", volt);
