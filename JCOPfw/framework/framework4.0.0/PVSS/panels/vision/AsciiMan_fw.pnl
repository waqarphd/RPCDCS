E"lockDpList()
{
dyn_string ds1 = dp.items;
dyn_string ds2 = dptypsel.items;
string      spr = getCatStr(\"general\",\"all\");
int pos;
bool b1, b2;



pos = dynContains(ds1, spr);
if ( pos >= 0)
  dynRemove(ds1, pos);
  
pos = dynContains(ds2, spr);
if ( pos >= 0)
  dynRemove(ds2, pos);
 
b1 = (dynlen(ds1)>0);   // b1 active
b2 = (dynlen(ds2)>0);   // b2 active

if ( b1 && b2)     // old selection  -> enable all
{
  b1 = FALSE;
  b2 = FALSE;
}

// disable if b1
dptypsel.enabled = !b1;
dptyp.enabled = !b1;
dptadd.enabled = !b1;
dptdel.enabled = !b1;

// disable if b2
dp.enabled = !b2;
dptext.enabled = !b2;
dpsel.enabled = !b2;
dpadd.enabled = !b2;
delsel.enabled = !b2;

}


buildHostConnectString(string &answer, bool bOther = FALSE)
{
dyn_string dsEventHosts = eventHost();
dyn_string dsDataHosts = dataHost();
unsigned udataPort = dataPort();
unsigned ueventPort = eventPort();
int hostNum =  1;

if ( bOther)
{
  if ( hostNum == 1)
    hostNum = 2;
}



answer = \" -data \"+dsDataHosts[hostNum]+ \":\"+udataPort+\" -event \"+dsEventHosts[hostNum]+\":\"+ueventPort;



}

