  string lang;
  int num;
  bool n, m, l;
  bool young, young2;

  getValue(\"\",\"number\",num);
  n = (num == 0);  // regular output
  m = (num == 1);  // ASCII Format2
  l = (num > 1);   // Powerconfig
  
  young = n || m;
   
  setMultiValue(\"langliste\",\"visible\",m,
                \"listelang\",\"visible\",m,
                \"langadd\",\"visible\",m,
                \"langdel\",\"visible\",m,
                \"rspr1\",\"visible\",m,
