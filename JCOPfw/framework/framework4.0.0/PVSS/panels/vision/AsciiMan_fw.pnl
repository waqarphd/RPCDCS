                \"FilterOptionen\", \"visible\", (n||m),
                \"Filter\", \"visible\", (n||m), 
                \"rout1\", \"visible\", (n||m),
                \"rout2\", \"visible\",(n||m) 
                );
 

if ( !young)  {                                              // -> Format 3 or 4 sedelect Filter 
          FilterOptionen.state(0) = FALSE;
          FilterOptionen.state(1) = FALSE;
					FilterOptionen.state(2) = FALSE;
					FilterOptionen.state(3) = FALSE;
					FilterOptionen.state(4) = FALSE;
					FilterOptionen.state(5) = FALSE;
					FilterOptionen.state(6) = FALSE;
					
    
           setMultiValue(\"tk\",\"visible\",0,
                        \"rk1\",\"visible\",0,
                        \"rk2\",\"visible\",0,
                        \"konfig\",\"visible\",0,
                        \"konfigliste\",\"visible\",0,
                        \"kadd\",\"visible\",0,
                        \"kdel\",\"visible\",0);
                        
                        
                        
        }
 
  if (young) 
    getValue(\"younger\",\"state\",0,young2);
  young &= young2;

  setMultiValue(\"YY\",\"visible\",young,
                \"TJ\",\"visible\",young,
                \"MM\",\"visible\",young,
                \"TM\",\"visible\",young,
                \"DD\",\"visible\",young,
                \"TT\",\"visible\",young,
                \"hh\",\"visible\",young,
                \"Ts\",\"visible\",young,
                \"mm\",\"visible\",young,
                \"Tm\",\"visible\",young,
                \"jetzt\",\"visible\",young);
  
  
  
  
  
  if (l)    // set only Masterdps 
  {
    dyn_string ds;
    int i;
    dyn_string dsTypes;
    
    dsTypes = dpNames(\"_mp_*\");
    
    for ( i= 1; i<= dynlen(dsTypes); i++)
