"main(int row, string column, string value)
{
	int i, rows;
	dyn_float df;
	dyn_string ds, systems;
	
	if($bEditFilter)
	{
		rows = this.lineCount();
		for(i=0; i<rows; i++)
			systems[i+1] = this.cellValueRC(i, \"systemName\");

    while(dynContains(systems, \"\") > 0)
      dynRemove(systems, dynContains(systems, \"\"));
//DebugN(systems);

		ChildPanelOnCentralModalReturn(\"fwAlarmHandling/fwAlarmHandlingSystemPicker.pnl\",
                                   \"System Picker\", makeDynString(\"$dsSelectedSystems:\" + systems),
                                   df, ds);

//DebugN(ds);
		if(df[1] > 0.5)
		{
			this.deleteAllLines();
      
    if(dynlen(ds) < 4)
    {
      this.vScrollBarMode = \"AlwaysOff\";
      ds[4] = \"\";
    }
    else
      this.vScrollBarMode = \"Auto\";

			this.appendLines(dynlen(ds), \"systemName\", ds);
		}
	}
}" 0
