"main()
{
	int i, length;
	string dollarValue;
	dyn_string deviceTypes;
	dyn_string exceptionInfo;
	dyn_dyn_anytype aesFilter;

	dollarValue = $dsDeviceTypes;
	deviceTypes = strsplit(dollarValue, \"|\");
	
	length = dynlen(deviceTypes);
	for(i=1; i<=length; i++)
	{
		deviceTypes[i] = strltrim(deviceTypes[i], \" \");
		deviceTypes[i] = strrtrim(deviceTypes[i], \" \");
	}
		
	this.items = deviceTypes;
	this.selectedPos = 1;

	dollarValue = $dsDpTypes;
	deviceTypes = strsplit(dollarValue, \"|\");
	
	length = dynlen(deviceTypes);
	for(i=1; i<=length; i++)
	{
		deviceTypes[i] = strltrim(deviceTypes[i], \" \");
		deviceTypes[i] = strrtrim(deviceTypes[i], \" \");
	}
		
	dpTypeList.items = deviceTypes;
	dpTypeList.selectedPos = 1;


	if($bEditFilter)
	{
		//wait for list to be completely filled
//		delay(0,200);

		_fwAlarmHandlingScreen_convertDollarToFilter($sCurrentFilter, aesFilter, exceptionInfo);
		fwAlarmHandlingScreen_showFilter(aesFilter, exceptionInfo);
		this.enabled = TRUE;
	}
}" 0

