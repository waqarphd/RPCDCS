"main()
{
	bool negateRange, ignoreOutside, inclusiveMin, inclusiveMax, doesExist;
	int minValue, maxValue;
	float fValue;
	string strValue, errorText;
	dyn_string exceptionInfo;

	strValue = this.text;

	if(sscanf(strValue,\" %f\", fValue) == 0 || sscanf(strValue,\" %f\", fValue) == -1)
	{
		errorText = \"$1:invalid format of value\";   
		ChildPanelOnCentral(\"vision/MessageInfo1\", \"\", makeDynString(errorText));
		return;
	}
	
	// Got a value, but is it any good?
	fwPvRange_get($sDpName + \".value\" , doesExist, minValue, maxValue,
						negateRange, ignoreOutside, inclusiveMin, inclusiveMax, exceptionInfo);
						
	if(dynlen(exceptionInfo) > 0)
		fwExceptionHandling_display(exceptionInfo);	

	if(doesExist)
	{
		if((fValue <= maxValue) && (fValue >= minValue))
		{
			dpSet($sDpName + \".value\", fValue);
		}
		else
		{
			errorText = \"$1:value outside pv_range\";
			ChildPanelOnCentral(\"vision/MessageInfo1\", \"\", makeDynString(errorText));
			dpGet($sDpName + \".value\", fValue);
			this.text = fValue;
			return;
		}
	}
	else
	{
		dpSet($sDpName + \".value\", fValue);
	}
}
