"main()
{
	dyn_string exceptionInfo;
	dyn_errClass err;

	dpConnect(\"work\", $sDpName + \".value:_online.._value\");

	err = getLastError();
	if (dynlen(err) > 0)
	{ 
		throwError(err);
		this.backCol = \"_dpdoesnotexist\";
		this.foreCol = \"_dpdoesnotexist\";
		fwException_raise(exceptionInfo, \"Error\", \"dpConnect() failed : \" + $sDpName + \".value\", \"\");
		fwExceptionHandling_display(exceptionInfo);
		return;
	}

	this.editable = FALSE;
}

work(string dp, float value)
{
	this.text = value;
}
