"main()
{
	dyn_string types;
	int i;
	
	types = fwFsm_getFwDeviceTypes();
	if(!dynlen(types))
	{
		types = dpTypes();
		for(i = 1; i <= dynlen(types); i++)
		{
			if(strpos(types[i],\"_\") == 0)
			{
				dynRemove(types, i);
				i--;
			}
			else
				break;
		}
	}
	dynAppend(types,\"Any Type\");
	setValue(\"\",\"text\",\"Any Type\");
	setValue(\"\",\"items\",types);
}" 0

"main()
{
	string type, name;
	dyn_string items, options;
	int i, pos;
	
	getValue(\"\",\"text\",type);
	if(type == \"Any Type\")
	{
		dynAppend(options, \"DP\");
	}
	else
	{
		dpCreate(\"_dummyDevice\",type);
		items = dpNames(\"_dummyDevice.**\",type);
		dpDelete(\"_dummyDevice\");
		for(i = 1; i <= dynlen(items); i++)
		{
			pos = strpos(items[i],\".\");
			name = 	substr(items[i],pos);
			if(name == \".\")
				name = \"\";
			dynAppend(options, \"DP\"+name);
		}
	}
	setValue(\"name\",\"text\",\"Named as:\");
	setValue(\"name\",\"items\",options);
}

