"main()
{
	string name;
	int editor, navigator, mode, index, i, pos;
	string root;
	dyn_string list, items, types, names;
	
	getValue(\"tree\",\"text\",name);
	if(name == \"\")
		PanelOffReturn(makeDynFloat(0), makeDynString());	
	getValue(\"mode\",\"state\",0,editor);
	getValue(\"mode\",\"state\",1,navigator);
	if(editor)
		mode = 1;
	if(navigator)
		mode++;
	getValue(\"root\",\"text\",root);
	if(root == \"Tree Name\")
		root = name;
	else if(root == \"System Name\")
		root = fwFsm_getSystemName()+\":\";
	else
		root = \"\";
	getValue(\"types\",\"items\",list);
	for(i = 1; i <= dynlen(list); i++)
	{
		items = strsplit(list[i],\"/\");
		dynAppend(types,items[1]);
		dynAppend(names,items[2]);
	}
	if((index = dynContains(types,\"Any Type\")))
	{
		dynClear(types);
		dynClear(names);
	}
//	DebugN(name, mode, root, types, names);
	fwTreeDisplay_createTree(name, mode, root, types, names);
	PanelOffReturn(makeDynFloat(1), makeDynString(name));
}
