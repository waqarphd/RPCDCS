E "dyn_string DefaultPars, DefaultValues;

initialize()
{
	int i, pos, width, height, nPars;
	dyn_string pars, items;
	string par, value, type, cmd;
	
	panelSize( myPanelName(), width, height);
	height = 0;
	if($2 != \"\")
	{
		items = strsplit($3, \"/\");
		cmd = items[1];
		fwFsm_readObjectActionParameters($1, $2, cmd, pars);
		height = 100;
DebugTN(\"Param panel\", $1, $2, $3, $4, $5, cmd, pars);
    if(!globalExists(\"FwFSMActionParameterValues\"))
      addGlobal(\"FwFSMActionParameterValues\", MAPPING_VAR);
	}
	else
	{
		fwFsm_readObjectParameters($1, pars);
		height = 150;
	}
	nPars = 0;
	for(i = 1; i <= dynlen(pars); i++)
	{
		items = strsplit(pars[i],\" \");
		par = items[2];
		type = items[1];
		value = \"\";
		if(dynlen(items) > 2)
		{		
			value = items[4];
			if((pos = strpos(value,\"\\\"\")) == 0)
			{
				value = substr(value,1,strlen(value)-2);
			}
		}
   if($2 != \"\")
   {
     DefaultPars[i] = par;
     DefaultValues[i] = value;
     if(mappingHasKey(FwFSMActionParameterValues,par))
     {
       value = FwFSMActionParameterValues[par];
       Default.visible = 1;
     }
   }
		if(strreplace(value, \"$dp=\", \"\") != 0)
		{
			dynRemove(pars, i);
			i--;
		}
		else
		{
			nPars++;
			setValue(\"param\"+i,\"text\",par);
			setValue(\"type\"+i,\"text\",type);
			setValue(\"value\"+i,\"text\",value);
			setValue(\"type\"+i,\"visible\",1);
			setValue(\"param\"+i,\"visible\",1);
			setValue(\"value\"+i,\"visible\",1);
		}
	}
	if($2 != \"\")
	{
		add_buttons(nPars);
	}
	setPanelSize( myModuleName(), myPanelName(), 0, width, nPars*30+height);

	setValue(\"type\"+i,\"text\",\"\");
	if(($2 == \"\")&& ($4 != \"\"))
	{
		fwUi_connectCurrentParameters(\"update_it\",$4, $5);
//			fwUi_getCurrentParameter($4, $5, par_dps[i], value);
	}
}

apply_defaults()
{
  int i;
  
  for(i = 1; i <= dynlen(DefaultValues); i++)
  {
    setValue(\"value\"+i,\"text\",DefaultValues[i]);
    mappingRemove(FwFSMActionParameterValues,DefaultPars[i]);
  }
}

int add_buttons(int nPars)
{
  string defaultValues;
  
  defaultValues = DefaultValues;
  addSymbol(myModuleName(),myPanelName(),\"fwFSM/ui/fwFsmParamsButtons.pnl\",\"paramButtons\",
    makeDynString($1, $2, $3, $4, $5, defaultValues),/*170*/0,nPars*30+50,0,1,1);
}

update_it(string dp, dyn_string pars)
{
	int i, pos;
	dyn_string items;
	string value;
	
	for(i = 1; i <= dynlen(pars); i++)
	{
		items = strsplit(pars[i],\" \");
		if(dynlen(items) == 3)
			value = \"\";
		else
			value = items[4];
		if((pos = strpos(value,\"\\\"\")) == 0)
		{
			value = substr(value,1,strlen(value)-2);
		}
		setValue(\"value\"+i,\"text\",value);
		setValue(\"value\"+i,\"editable\",0);
	}
}" 0
