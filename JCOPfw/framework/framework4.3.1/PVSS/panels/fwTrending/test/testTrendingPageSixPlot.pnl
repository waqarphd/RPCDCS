			x, y, 1.0, 1.0, 1.0);
	addSymbol(myModuleName(), myPanelName(), \"fwTrending/fwTrendingSixPlot.pnl\", ref+\"trend\", dsCurveParameters,
			x, y+20, 1.0, 1.0, 1.0);

/*	addSymbol(myModuleName(), myPanelName(), \"objects/fwTrending/fwTrendingFaceplate.pnl\", ref, dsCurveParameters,
			x, y, 1.0, 1.0, 1.0);
*/
