    _dpesVal[i] = getDpeFromTextField(\"DPTextField\" + _num);
		getValue(\"LegendTextField\" + i, \"text\", _legendVal[i]);
		getValue(\"ColorButton\" + i, \"backCol\", _colorsVal[i]);
