string getDpeFromTextField(string textField)
{
  string sText, sDpe;
  
  getValue(textField, \"text\", sText);
  
  if(fwTrending_isAlias(sText))
  {
    sDpe = fwTrending_createAliasRepresentation(sText);
  }
  else
  {
    sDpe = sText;
  }

  return sDpe;
}
